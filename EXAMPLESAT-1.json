{
	"json_format_version" : 1,

	"transmitters" : [{
		"transmitter_name" : "main transmitter",
		"frequency_mhz"    : 437.385,
		"modulation"       : "2FSK",
		"decoder"          : "AX25",
		"callsign"         : "SUPERSAT // this field only with AX25",

		"packets" : {
			"adc_packet" : {
				"__TYPE__" : 0x5667,

				"fields"   : {
					"adc1"                    : "int32",
					"__TYPE__"                : "int16",

					"status_2_bytes"          : {
						0: "boot_on_bit",
						2: "status_bit",
						9: "tra_bit"
					},

					"len"                     : "uint32",
					"data_with_len_specified" : "array{len}",
					"data_till_eof"           : "array{}"
				}
			}
		}
	}]
}
